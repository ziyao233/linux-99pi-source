};
&sdio1{
    status = "disabled";
    pinctrl-names = "default";
    pinctrl-0 = <&sdio1_6bit>, <&sdio1_10bit_m0>;
};

&i2c0{
    status = "disabled";
    clock-frequency = <400000>;
    pinctrl-names = "default";
    pinctrl-0 = <&i2c0_mux_m0>;
};
&i2c1{
	status = "disabled";
    clock-frequency = <400000>;
    pinctrl-names = "default";
    pinctrl-0 = <&i2c1_mux_m0>;
};
&i2c2{
	status = "okay";
    clock-frequency = <400000>;
    pinctrl-names = "default";
    pinctrl-0 = <&i2c2_mux_m1>;

	gt9xx-i2c@14{
    	status = "okay";
    	compatible = "goodix,gt9xx";
    	reg = <0x14>;

    	reset-gpios = <&gpa6 8 0>;
    	irq-gpios = <&gpa6 7 0>;
    	irq-flags = <IRQ_TYPE_EDGE_FALLING>;
    	goodix,type-a-report = <0>;
    	goodix,create-wr-node = <1>;
    	goodix,wakeup-with-reset = <0>;
    	goodix,resume-in-workqueue = <0>;
    	goodix,int-sync = <1>;
    	goodix,swap-x2y = <0>;
    	goodix,esd-protect = <0>;
	goodix,auto-update = <1>;
    	goodix,auto-update-cfg = <0>;
    	goodix,power-off-sleep = <0>;
    	goodix.pen-suppress-finger = <0>;
    	goodix,cfg-group0 = [
        	53 D0 02 00 05 05 F5 D5 21 48 2D 0F 5A 41 0E 05 00 00 32
            	32 20 00 05 14 14 1A 14 8B 2B 00
            	];
	};

};
&i2c3{
	status = "okay";
    clock-frequency = <400000>;
    pinctrl-names = "default";
    pinctrl-0 = <&i2c3_mux_m1>;

	codec@10{
		compatible = "es8323";
		reg = <0x10>;
	};

};

&spi0{
	status = "disable";
    pinctrl-names = "default";
    pinctrl-0 = <&spi0_6bit_f0>;

	norflash@0 {
       reg = <0>;
       spi-rx-bus-width = <4>;
       spi-max-frequency = <25000000>;
       #address-cells = <1>;
       #size-cells = <1>;
       partitions {
               compatible = "fixed-partitions";
           #address-cells = <1>;
           #size-cells = <1>;

           boot@0 {
               label = "boot";
               reg = <0x0000000 0x100000>;
               /*read-only;*/
           };

           kernel@100000 {
               label = "kernel";
               reg = <0x100000 0x600000>;
           };

           rootfs@700000 {
               label = "rootfs";
               reg = <0x700000 0x1000000>;
           };
       };

    };

};
&spi2{
	status = "okay";
    pinctrl-names = "default";
    pinctrl-0 = <&spi2_pin>;
};
